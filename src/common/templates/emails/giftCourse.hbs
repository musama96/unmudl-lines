  <div style="font-family:'Poppins';position:static;margin-left:154.35px;margin-top:0px;width:348px;border-left:0.5px solid #b8bfbb;border-right:0.5px solid #b8bfbb;border-bottom:0.5px solid #b8bfbb;border-radius:3.5px;display:inline-block" class="cls_006">
    <div style="font-family:'Poppins';position:static;margin-left:7.35px;margin-top:3px" class="cls_006"><span class="cls_006">{{courseTitle}}</span></div>
    {{#if courseDate}}
    <div style="font-family:'Poppins';position:static;margin-left:7.35px;margin-top:9px" class="cls_008"><span class="cls_008">{{courseDate}}</span></div>
    {{/if}}
