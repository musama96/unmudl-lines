    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/schedule": "^0.2.0",
    "@nestjs/swagger": "^4.6.0",
    "@types/dotenv": "^8.2.0",
    "atob": "^2.1.2",
    "aws-sdk": "^2.829.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "btoa": "^1.2.1",
    "cache-manager": "^3.4.0",
