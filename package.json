    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "flatted": "^3.1.0",
    "google-auth-library": "^5.9.2",
    "handlebars": "^4.7.6",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.8.7",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.13",
    "nodemailer-ses-transport": "^1.5.1",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
